#division
  box-sizing border-box
  width 100%
  padding 30px
  display flex
  flex-direction column
  align-items center
  background white
  .div_header
    width 528px
    height 192px
    >img
      width 100%
      height 100%
  .div_content
    width 100%
    .dc_ul
      width 100%
      overflow hidden
      .dc_li
        float left
        width 33.333333333%
        height 200px
        margin-top 35px
        .dcl_img
          width 144px
          height 144px
          >img
            width 100%
            height 100%
        .dcl_text
          width 144px
          text-align center
